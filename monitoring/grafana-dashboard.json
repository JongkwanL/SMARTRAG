{
  "dashboard": {
    "id": null,
    "title": "SmartRAG Monitoring Dashboard",
    "tags": ["smartrag", "rag", "llm"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "5s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Request Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(smartrag:request_rate_5m)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "color": {
              "mode": "palette-classic"
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Error Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(smartrag:error_rate_5m)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 0.01
                },
                {
                  "color": "red",
                  "value": 0.05
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Response Time (P95)",
        "type": "stat",
        "targets": [
          {
            "expr": "smartrag:response_time_p95_5m",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 1.2
                },
                {
                  "color": "red",
                  "value": 2.0
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 12,
          "y": 0
        }
      },
      {
        "id": 4,
        "title": "Cache Hit Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "smartrag:cache_hit_rate_5m",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "red",
                  "value": null
                },
                {
                  "color": "yellow",
                  "value": 0.5
                },
                {
                  "color": "green",
                  "value": 0.7
                }
              ]
            }
          }
        },
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 18,
          "y": 0
        }
      },
      {
        "id": 5,
        "title": "Requests Over Time",
        "type": "timeseries",
        "targets": [
          {
            "expr": "sum(rate(smartrag_requests_total[1m])) by (endpoint)",
            "refId": "A",
            "legendFormat": "{{endpoint}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 6,
        "title": "Response Time Distribution",
        "type": "timeseries",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(smartrag_request_duration_seconds_bucket[5m]))",
            "refId": "A",
            "legendFormat": "P50"
          },
          {
            "expr": "histogram_quantile(0.95, rate(smartrag_request_duration_seconds_bucket[5m]))",
            "refId": "B",
            "legendFormat": "P95"
          },
          {
            "expr": "histogram_quantile(0.99, rate(smartrag_request_duration_seconds_bucket[5m]))",
            "refId": "C",
            "legendFormat": "P99"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "s"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 8
        }
      },
      {
        "id": 7,
        "title": "LLM Metrics",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(smartrag_llm_requests_total[5m])",
            "refId": "A",
            "legendFormat": "LLM Requests/sec"
          },
          {
            "expr": "rate(smartrag_llm_tokens_total[5m])",
            "refId": "B",
            "legendFormat": "Tokens/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 8,
        "title": "Vector Store Metrics",
        "type": "timeseries",
        "targets": [
          {
            "expr": "smartrag_vector_store_documents_total",
            "refId": "A",
            "legendFormat": "Total Documents"
          },
          {
            "expr": "rate(smartrag_embeddings_generated_total[5m])",
            "refId": "B",
            "legendFormat": "Embeddings/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 16
        }
      },
      {
        "id": 9,
        "title": "System Resources",
        "type": "timeseries",
        "targets": [
          {
            "expr": "smartrag_memory_usage_bytes / smartrag_memory_limit_bytes",
            "refId": "A",
            "legendFormat": "Memory Usage %"
          },
          {
            "expr": "rate(smartrag_cpu_usage_seconds_total[5m])",
            "refId": "B",
            "legendFormat": "CPU Usage %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent"
          }
        },
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 24
        }
      },
      {
        "id": 10,
        "title": "Error Breakdown",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(smartrag_requests_total{status=~\"4..\"}[5m])",
            "refId": "A",
            "legendFormat": "4xx Errors"
          },
          {
            "expr": "rate(smartrag_requests_total{status=~\"5..\"}[5m])",
            "refId": "B",
            "legendFormat": "5xx Errors"
          },
          {
            "expr": "rate(smartrag_embedding_generation_errors_total[5m])",
            "refId": "C",
            "legendFormat": "Embedding Errors"
          },
          {
            "expr": "rate(smartrag_llm_errors_total[5m])",
            "refId": "D",
            "legendFormat": "LLM Errors"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 24
        }
      },
      {
        "id": 11,
        "title": "Cache Performance",
        "type": "timeseries",
        "targets": [
          {
            "expr": "rate(smartrag_cache_hits_total[5m])",
            "refId": "A",
            "legendFormat": "Cache Hits/sec"
          },
          {
            "expr": "rate(smartrag_cache_misses_total[5m])",
            "refId": "B",
            "legendFormat": "Cache Misses/sec"
          },
          {
            "expr": "smartrag_cache_size_bytes",
            "refId": "C",
            "legendFormat": "Cache Size (bytes)"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 32
        }
      }
    ],
    "templating": {
      "list": [
        {
          "name": "interval",
          "type": "interval",
          "options": [
            {"text": "1m", "value": "1m"},
            {"text": "5m", "value": "5m"},
            {"text": "15m", "value": "15m"},
            {"text": "1h", "value": "1h"}
          ],
          "current": {
            "text": "5m",
            "value": "5m"
          }
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "Prometheus",
          "expr": "smartrag_deployment_timestamp",
          "textFormat": "Deployment",
          "titleFormat": "New Deployment",
          "iconColor": "rgba(0, 211, 255, 1)"
        }
      ]
    }
  }
}